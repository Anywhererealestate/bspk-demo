{
    "name": "bspk-demo",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "license": "CC-BY-4.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Anywhererealestate/bspk-demo"
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "build:dev": "vite-node ./.scripts/build.ts",
        "build": "vite-node ./.scripts/build.ts",
        "meta": "vite-node ./.scripts/meta.ts",
        "meta:colors": "vite-node ./.scripts/tasks/generateColorData.ts",
        "dev": "vite-node ./.scripts/dev.ts",
        "lint": "eslint && tsc --noemit",
        "lint:fix": "eslint --fix",
        "lint:css": "npx stylelint '**/*.scss'",
        "lint:css:fix": "npx stylelint '**/*.scss' --fix",
        "prepare": "husky",
        "preview:local": "vite-node ./.scripts/preview.ts local",
        "preview:prod": "vite-node ./.scripts/preview.ts prod",
        "preview:dev": "vite-node ./.scripts/preview.ts dev",
        "test:dev": "TEST_URL=http://localhost:8675 playwright test",
        "test:local:setup": "vite-node ./.scripts/build-test-local.ts",
        "test:local": "TEST_URL=http://localhost:8080 playwright test",
        "tasks": "vite-node ./.scripts/tasks.ts",
        "pr": "npm run lint && npm run lint:css && npm run build && git push --set-upstream origin $(git symbolic-ref --short HEAD) && open \"https://github.com/Anywhererealestate/bspk-demo/compare/dev...$(git symbolic-ref --short HEAD)\""
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tslib": "^2.8.1"
    },
    "dependencies": {
        "@axe-core/playwright": "^4.10.1",
        "@bspk/icons": "^1.0.4",
        "@bspk/styles": "^1.0.2",
        "@bspk/ui": "^1.3.21",
        "axe-core": "^4.10.2",
        "highlight.js": "^11.11.1",
        "markdown-to-jsx": "^7.7.4",
        "marked": "^15.0.6",
        "react-live": "^4.1.8",
        "react-router-dom": "^7.0.1",
        "store": "^2.0.12"
    },
    "devDependencies": {
        "@cspell/eslint-plugin": "^9.4.0",
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^9.39.1",
        "@playwright/test": "^1.50.1",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint-plugin-jsx-a11y": "^6.10.1",
        "@types/node": "^22.13.4",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.5",
        "@types/store": "^2.0.5",
        "@vitejs/plugin-react": "^4.3.3",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "globals": "^15.11.0",
        "http-server": "^14.1.1",
        "husky": "^9.1.7",
        "json-schema": "^0.4.0",
        "prettier": "^3.4.2",
        "prettier-plugin-jsdoc": "^1.3.0",
        "puppeteer": "^23.11.1",
        "sass": "^1.82.0",
        "sass-embedded": "^1.81.0",
        "simple-git": "^3.27.0",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "tsx": "^4.19.4",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.49.0",
        "typescript-json-schema": "^0.65.1",
        "vite": "^6.3.5",
        "vite-node": "^3.2.3",
        "vite-plugin-run": "^0.6.1",
        "watcher": "^2.3.1"
    }
}
