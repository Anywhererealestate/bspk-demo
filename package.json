{
  "name": "bspk-demo",
  "private": true,
  "version": "1.0.0-alpha",
  "type": "module",
  "license": "CC-BY-ND-4.0",
  "scripts": {
    "build": "npm run meta && tsc -b && vite build",
    "dev:demo": "npm run meta && rm -rf node_modules/.vite && vite --open",
    "dev:setup": "vite-node ./.scripts/setup-dev.ts",
    "dev": "npm run dev:setup && npm run dev:demo | npm run dev:watch",
    "dev:watch": "vite-node ./.scripts/watch-dev.ts",
    "lint": "eslint && tsc --noemit",
    "lint:fix": "eslint --fix",
    "lint:phases": "vite-node .scripts/lint-phases.ts",
    "lint:types": "vite-node .scripts/lint-types.ts",
    "lint:styles": "vite-node .scripts/lint-style.ts",
    "meta": "vite-node ./.scripts/create-meta.tsx",
    "parser": "vite-node .scripts/js-doc-parser.ts",
    "prepare": "husky",
    "prod": "npm run build && vite preview --open",
    "update-ui": "vite-node ./.scripts/update-ui.ts && npm run changelog",
    "test": "TEST_PORT=8675 playwright test",
    "test:prod": "TEST_PORT=8080 playwright test",
    "changelog": "vite-node ./.scripts/changelog.ts"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@bspk/icons": "^2.3.5",
    "@bspk/ui": "^2.3.99",
    "@emotion/css": "^11.13.5",
    "@types/react": "^18.3.12",
    "axe-core": "^4.10.2",
    "highlight.js": "^11.11.1",
    "markdown-to-jsx": "^7.7.4",
    "marked": "^15.0.6",
    "react-router-dom": "^7.0.1",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@bspk/styles": "^1.5.10",
    "@cspell/eslint-plugin": "^8.17.5",
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@types/store": "^2.0.5",
    "@vitejs/plugin-react": "^4.3.3",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "prettier-plugin-jsdoc": "^1.3.0",
    "puppeteer": "^23.11.1",
    "sass": "^1.82.0",
    "sass-embedded": "^1.81.0",
    "simple-git": "^3.27.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.13.0",
    "typescript-json-schema": "^0.65.1",
    "vite": "^5.4.12",
    "vite-node": "^2.1.8",
    "watcher": "^2.3.1"
  }
}